<section id="section-4">
    <h1><img src="img/section-4/section4.png" alt="Wings" style="width: 70%;"></h1>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    </head>

    <body>

        <div class="container">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>
                    <li data-target="#myCarousel" data-slide-to="4"></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="img/section-4/Broadway Theatre Image.jpg" alt="Broadway"
                            style="width:100%;height:300px;">
                        <section>
                            <div>
                                <h2>Broadway style="color:#fff";</h2>
                                <p>Gerald Schoenfeld Theatre 236 West 45th Street, NYC</p>
                                <button>Ticket Sales</button>
                            </div>
                        </section>
                    </div>

                    <div class="item">
                        <img src="img/section-4/Toronto Theatre Image.jpg" alt="Toronto"
                            style="width:100%;height:300px;">
                        <section>
                            <div>
                                <h2>Toronto</h2>
                                <p>Royal Alexandra Theatre 260 King St. W, Toronto ON M5V 1H9</p>
                                <button>Ticket Sales</button>
                            </div>
                        </section>
                    </div>

                    <div class="item">
                        <img src="img/section-4/London Theatre Image.jpg" alt="London" style="width:100%;height:300px;">
                        <section>
                            <div>
                                <h2>London</h2>
                                <p>Phoenix Theatre Charing Cross Road, London WC2H 0JP</p>
                                <button>Ticket Sales</button>
                            </div>
                        </section>
                    </div>

                    <div class="item">
                        <img src="img/section-4/Melbourne Theatre Image.jpg" alt="Melbourne"
                            style="width:100%;height:300px;">
                        <section>
                            <div>
                                <h2>Melbourne</h2>
                                <p>Comedy Theatre 240 Exhibition St Melbourne VIC 3000, Australia</p>
                                <button>Ticket Sales</button>
                            </div>
                        </section>
                    </div>

                    <div class="item">
                        <img src="img/section-4/Sydney Theatre Image.jpg" alt="Sydney" style="width:100%;height:300px;">
                        <section>
                            <div>
                                <h2>Sydney</h2>
                                <p>Capitol Theatre, 13 Campbell St, Haymarket NSW 2000</p>
                                <button>Ticket Sales</button>
                            </div>
                        </section>
                    </div>
                </div>

                <!-- Left and right controls -->
                <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>

    </body>
</section>